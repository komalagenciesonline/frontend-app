{
  "expo": {
    "name": "Komal Agencies",
    "slug": "komal-agencies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "komal-agencies",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Komal Agencies - A comprehensive business management app for handling items, orders, and retailer operations.",
    "privacy": "public",
    "githubUrl": "https://github.com/yourusername/komal-agencies",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.komalagencies.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.komalagencies.app",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a14716c1-554f-4e42-af09-607ce24396f3"
      }
    }
  }
}
